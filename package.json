{
  "name": "lessons-site-generator",
  "version": "0.0.1",
  "license": "CC-BY-4.0",
  "repository": "github:bridge-school/lessons-site-generator",
  "private": true,
  "scripts": {
    "start": "npx nodemon -w static.config.js --exec \"npx react-static start\"",
    "start:prod": "npx nodemon -w static.config.js -w src --exec \"npm run build && npm run serve\"",
    "build": "npx react-static build",
    "pages": "npx react-static build",
    "deploy": "npm run pages && npx gh-pages -d dist",
    "serve": "npx serve dist -p 3000",
    "lint:js": "find src -name '*.js' | xargs npx eslint -c .eslintrc.js --fix",
    "lint:css": "find src -name '*.css' | xargs npx stylelint --fix --config .stylelintrc",
    "lint": "npx concurrently -k \"npm run lint:js\" \"npm run lint:css\"",
    "precommit": "npx lint-staged"
  },
  "dependencies": {
    "commander": "^2.12.2",
    "fs-extra": "^5.0.0",
    "fs-extra-promise": "^1.0.1",
    "glob-promise": "^3.3.0",
    "gray-matter": "^3.1.1",
    "gulp": "^3.9.1",
    "gulp-debug": "^3.1.0",
    "gulp-flatmap": "^1.0.0",
    "gulp-git": "^2.4.2",
    "gulp-if": "^2.0.2",
    "gulp-ignore": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.7",
    "material-components-web": "^0.27.0",
    "mustache": "^2.3.0",
    "mz": "^2.7.0",
    "next": "^4.2.1",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-static": "^4.7.0-beta.0",
    "react-svg-loader": "^2.1.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "require-dir": "^0.3.2",
    "rmwc": "0.0.1-rc12",
    "through2": "^2.0.3",
    "vinyl": "^2.1.0",
    "vinyl-transform": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-tools": "^1.0.10",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "serve": "^6.4.3",
    "stream-browserify": "^2.0.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  }
}
