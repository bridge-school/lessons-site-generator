{
  "name": "gatsby-starter",
  "version": "1.0.0",
  "description": "Gatsby 1.0 starter",
  "author": "fabien0102 <fabien0102@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "lint": "npm run lint:ts && npm run lint:md && npm run lint:js && npm run lint:css",
    "lint:ts": "tslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "lint:md": "remark .",
    "lint:js": "xo",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:scss": "stylelint src/**/*.scss --syntax scss",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "graphql-types": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./src/graphql-types.d.ts",
    "precommit": "lint-staged",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "dependencies": {
    "@types/prop-types": "^15.5.2",
    "@types/ramda": "^0.25.6",
    "@types/react": "16.0.7",
    "@types/react-dom": "^15.5.5",
    "@types/react-helmet": "5.0.0",
    "@types/react-redux": "^5.0.9",
    "@types/react-router": "^4.0.14",
    "@types/react-router-dom": "^4.0.8",
    "babel-eslint": "^7.2.1",
    "chalk": "^2.3.0",
    "change-case": "^3.0.1",
    "circular-dependency-plugin": "^4.2.1",
    "codeclimate-test-reporter": "^0.4.1",
    "commander": "^2.12.2",
    "entities": "^1.1.1",
    "fs-extra-promise": "^1.0.1",
    "gatsby": "latest",
    "gatsby-link": "^1.6.31",
    "gatsby-plugin-glamor": "latest",
    "gatsby-plugin-google-analytics": "latest",
    "gatsby-plugin-manifest": "latest",
    "gatsby-plugin-offline": "latest",
    "gatsby-plugin-sharp": "latest",
    "gatsby-plugin-typescript": "latest",
    "gatsby-remark-autolink-headers": "latest",
    "gatsby-remark-copy-linked-files": "latest",
    "gatsby-remark-images": "latest",
    "gatsby-remark-prismjs": "latest",
    "gatsby-source-filesystem": "latest",
    "gatsby-transformer-json": "latest",
    "gatsby-transformer-remark": "latest",
    "gatsby-transformer-sharp": "latest",
    "graphql-code-generator": "^0.5.2",
    "gray-matter": "^2.1.1",
    "gulp": "^3.9.1",
    "gulp-debug": "^3.1.0",
    "gulp-file": "^0.3.0",
    "gulp-filter": "^5.0.1",
    "gulp-foreach": "^0.1.0",
    "gulp-git": "^2.4.2",
    "gulp-if": "^2.0.2",
    "gulp-ignore": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-transform": "^3.0.5",
    "gulp-util": "^3.0.8",
    "jest-cli": "^21.2.1",
    "lodash": "^4.17.4",
    "lodash.pick": "^4.4.0",
    "lodash.zip": "^4.2.0",
    "mustache": "^2.3.0",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "5.0.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "require-dir": "^0.3.2",
    "semantic-ui-react": "^0.74.2",
    "slash": "1.0.0",
    "through2": "^2.0.3",
    "ts-loader": "^2.3.7",
    "tslib": "^1.8.1",
    "typescript": "2.5.3",
    "vinyl": "^2.1.0",
    "vinyl-transform": "^1.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.9",
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.15",
    "@types/react-test-renderer": "^15.4.3",
    "@types/webpack-env": "^1.13.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint-config-xo-react": "0.11.1",
    "eslint-plugin-react": "6.10.3",
    "flat": "^2.0.1",
    "highlight.js": "^9.12.0",
    "husky": "0.13.3",
    "jest": "^19.0.2",
    "lint-staged": "3.4.0",
    "mkdirp": "^0.5.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "16",
    "remark": "^8.0.0",
    "remark-cli": "^3.0.1",
    "remark-frontmatter": "^1.1.0",
    "remark-html": "^6.0.1",
    "remark-parse": "^4.0.0",
    "remark-preset-lint-recommended": "^2.0.0",
    "remark-stringify": "^4.0.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "ts-jest": "19.0.8",
    "ts-lint": "4.5.1",
    "xo": "^0.18.1"
  },
  "keywords": [
    "gatsby",
    "starter"
  ],
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PREFIX_LINKS__": ""
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/templates/",
      "/.cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ],
    "*.md": [
      "node tools/update-post-date.js",
      "remark -o",
      "git add"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "engines": {
    "node": ">= 8",
    "npm": " >= 5"
  },
  "xo": {
    "extends": "xo-react/space",
    "space": true,
    "rules": {
      "no-case-declarations": 0,
      "default-case": 0
    },
    "overrides": [
      {
        "files": "gulpfile.js",
        "rules": {
          "unicorn/no-process-exit": 0
        }
      }
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
